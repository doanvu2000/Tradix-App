<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginVertical="10dp"
        android:layout_weight="1"
        tools:ignore="MissingConstraints">

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/vertical_1"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.16" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/vertical_0"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.05" />

        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:backgroundTint="#F137CC"
            android:scaleType="center"
            android:src="@drawable/icons_8_alarm"
            app:fabSize="mini"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="@id/vertical_0"
            app:layout_constraintRight_toRightOf="@id/vertical_1"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/tvAlert"
            style="@style/TextStyle72"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:gravity="center|left"
            android:paddingStart="10dp"
            android:text="Alerts"
            app:layout_constraintLeft_toLeftOf="@id/vertical_1"
            app:layout_constraintRight_toRightOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        tools:ignore="MissingConstraints">

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/vertical_2"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_marginVertical="10dp"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.16" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/vertical_20"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_marginVertical="10dp"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.05" />

        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:backgroundTint="#F137CC"
            android:scaleType="center"
            android:src="@drawable/icons_8_left_and_right_arrows"
            app:fabSize="mini"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="@id/vertical_20"
            app:layout_constraintRight_toRightOf="@id/vertical_2"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/tvPrediction"
            style="@style/TextStyle72"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:gravity="center|left"
            android:paddingStart="10dp"
            android:text="Predictions"
            app:layout_constraintLeft_toLeftOf="@id/vertical_2"
            app:layout_constraintRight_toRightOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginVertical="10dp"
        android:layout_weight="1"
        tools:ignore="MissingConstraints">

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/vertical_3"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.16" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/vertical_30"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.05" />

        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:backgroundTint="#F137CC"
            android:scaleType="center"
            android:src="@drawable/icons_8_pin"
            app:fabSize="mini"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="@id/vertical_30"
            app:layout_constraintRight_toRightOf="@id/vertical_3"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/tvSaved"
            style="@style/TextStyle72"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:gravity="center|left"
            android:paddingStart="10dp"
            android:text="Saved elements"
            app:layout_constraintLeft_toLeftOf="@id/vertical_3"
            app:layout_constraintRight_toRightOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginBottom="10dp"
        android:layout_weight="1"
        tools:ignore="MissingConstraints">

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/vertical_4"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.16" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/vertical_40"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.05" />

        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:backgroundTint="#F137CC"
            android:scaleType="center"
            android:src="@drawable/icons_8_no_entry"
            app:fabSize="mini"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="@id/vertical_40"
            app:layout_constraintRight_toRightOf="@id/vertical_4"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/tvRemove"
            style="@style/TextStyle72"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:gravity="center|left"
            android:paddingStart="10dp"
            android:text="Remove Ads"
            app:layout_constraintLeft_toLeftOf="@id/vertical_4"
            app:layout_constraintRight_toRightOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>

</LinearLayout>